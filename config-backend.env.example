#HYPHE_PROXY_HOST=
#HYPHE_PROXY_PORT=

HYPHE_MAXDEPTH=3
HYPHE_DOWNLOAD_DELAY=1
HYPHE_STORE_CRAWLED_HTML=false
HYPHE_MAX_SIM_REQ=12
HYPHE_HOST_MAX_SIM_REQ=1

HYPHE_TRAPH_KEEPALIVE=1800
